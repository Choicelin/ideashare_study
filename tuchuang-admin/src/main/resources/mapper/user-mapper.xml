<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mini.ideashare.cms.dao.UserDAO">
    <resultMap id="BaseResultMap" type="mini.ideashare.cms.model.User">
       <result property="id" column="id"></result>
        <result property="openId" column="open_id"></result>
        <result property="userName" column="user_name"></result>
        <result property="userImage" column="user_image"></result>
        <result property="createTime" column="create_time"></result>
        <result property="lastLoginTime" column="last_login_time"></result>
    </resultMap>
    <insert id="insert">
        insert into users() VALUES(NULL,#{user.userName},#{user.userImage},#{user.openId},#{user.createTime},#{user.lastLoginTime})
    </insert>

</mapper>
